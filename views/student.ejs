  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Student Dashboard</title>
  <script defer src="/scripts/student.js"></script>
</head>
<body>
  <h1>Student Dashboard</h1>
  <h2>Assignments</h2>
  <table border="1">
    <thead>
      <tr>
        <th>Title</th>
        <th>Action</th>
      </tr>
    </thead>
